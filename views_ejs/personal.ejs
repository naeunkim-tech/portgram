    <title>개인 페이지</title>
    <link rel="stylesheet" href="./css/personal.css" />
    <link rel="stylesheet" href="./css/header.css" />
  </head>
  <body>
    <main id="main">
      <div id="personal_page">
        <div id="profile">
          <!-- <div class="img"> -->
          <img
            class="profile_img"
