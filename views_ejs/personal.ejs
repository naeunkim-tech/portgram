          </div> -->

          <details open>
            <summary>학력</summary>
            <div markdown="1" class="education">
              <div id="button">
                <button id="addEdu" class="btn">ADD</button>
              </div>
              <form id="educationForm">
                <div class="add"></div>
              </form>
              <div id="addedEducation"></div>
            </div>
          </details>

          <!-- <div class="award">
