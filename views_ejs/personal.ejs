            <div id="addedAward"></div>
          </div> -->

          <details open>
            <summary>수상이력</summary>
            <div markdown="1" class="award">
              <div id="button">
                <button id="addAward" class="btn">ADD</button>
              </div>
              <form id="awardForm">
                <div class="add"></div>
              </form>
              <div id="addedAward"></div>
            </div>
          </details>

